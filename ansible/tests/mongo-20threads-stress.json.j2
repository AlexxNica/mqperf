{
    "name": "mongo-20threads-stress-$runid",
    "mq_type": "Mongo",
    "sender_threads": 20,
    "msg_count_per_thread": 300000,
    "msg_size": 100,
    "max_send_msg_batch_size": 10,
    "receiver_threads": 20,
    "receive_msg_batch_size": 10,
    "broker_hosts": {{ groups['mongo']|map('extract', hostvars, 'ec2_private_ip_address')|list|to_json }}
    "mq": {
        "write_concern": "replica"
    }
}
